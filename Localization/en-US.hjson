					#[Rare Spell Reagent]{ItemLink:RareSpellReagent}
					#[Mythical Spell Reagent]{ItemLink:MythicalSpellReagent}
					#[Silver Mirror]{ItemLink:SilverMirror}
					
					#[Spell tomes]{Header2}
					#[Spellwright Teachings]{ItemLink:SpellwrightTeachings}
					#[Beginner Spell Tome]{ItemLink:BeginnerSpellTome}
					#[Advanced Spell Tome]{ItemLink:AdvancedSpellTome}
					#[Supreme Spell Tome]{ItemLink:SupremeSpellTome}
					
